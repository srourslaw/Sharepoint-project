# MSAL Configuration - Copy to .env.local and fill in your values
# These values should match your Azure AD app registration

# Azure AD Application (Client) ID
VITE_AZURE_CLIENT_ID=fd3b804c-5ac4-4e00-8359-f6712fc1e634

# Azure AD Directory (Tenant) ID
VITE_AZURE_TENANT_ID=a68d3c04-09fe-4a33-a02c-e880c1a7504d

# Redirect URI - should match what's registered in Azure AD
VITE_AZURE_REDIRECT_URI=http://localhost:8080

# API Base URL (optional - for hybrid backend/frontend auth)
VITE_API_BASE_URL=http://localhost:3001
